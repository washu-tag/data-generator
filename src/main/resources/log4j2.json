{
  "Configuration": {
    "Appenders": {
      "Console": {
        "name": "CONSOLE",
        "PatternLayout": {
          "pattern": "%d{HH:mm:ss.SSS} [%p] - %m%n"
        }
      }
    },
    "Loggers": {
      "Logger": [
        {
          "name": "org.apache.spark",
          "level": "WARN",
          "AppenderRef": [
            {
              "ref": "CONSOLE"
            }
          ]
        },
        {
          "level": "INFO",
          "AppenderRef": [
            {
              "ref": "CONSOLE"
            }
          ]
        }]
    }
  }
}